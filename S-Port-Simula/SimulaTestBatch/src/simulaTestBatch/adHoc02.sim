Simulation begin 
         
	ref(Prime) x;      
	integer tm;  
     
	Process class Prime(id); integer id;  
	begin integer tm;  
	end; 
   
	x :- new Prime(444);
	activate x at 313;  
	
	tm := time;  
	outtext("Time="); outint(tm,0); outimage; 
	tm := x.evtime;  
	outtext("EvTime="); outint(tm,0); outimage; 
    
end
